	"printUrl": "https://demo.geo-solutions.it/geoserver/pdf/info.json",
  "themePrefix": "mockups",
	"translationsPath": "./assets/translations",
	"plugins": {
      "index": [
        "Archive"
      ],
			"query-builder": [
